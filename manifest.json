{
  "name": "Unsubtitle for Netflix",
  "description": "Remove forced Netflix subtitles.",
  "version": "3.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://*.netflix.com/*"
  ],
  "action": {
    "default_title": "Remove subtitles",
    "default_icon": "19.png"
  },
  "icons": {
    "16": "16.png",
    "19": "19.png",
    "24": "24.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "manifest_version": 3
}