{
  "name": "Unsubtitle for Netflix",
  "description": "Remove forced Netflix subtitles.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Remove subtitles",
      "default_icon": "19.png"
  },
  "icons": {
      "16": "16.png",
      "19": "19.png",
      "24": "24.png",
      "32": "32.png",
      "48": "48.png",
      "128": "128.png"
  },
  "manifest_version": 2
}
